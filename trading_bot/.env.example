# Binance Futures Testnet API Credentials
# Copy this file to .env and fill in your credentials

# Your Binance API Key
API_KEY=your_api_key_here

# Your Binance API Secret
API_SECRET=your_api_secret_here

# Base URL for Binance Futures Testnet (default: https://testnet.binancefuture.com)
BASE_URL=https://testnet.binancefuture.com

# Default leverage for futures trading (default: 1)
DEFAULT_LEVERAGE=1
